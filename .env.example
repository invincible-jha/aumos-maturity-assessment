# AumOS Maturity Assessment — Environment Variables
# Copy to .env and customize for local development

# ── Standard AumOS Settings ─────────────────────────────────────────────────
AUMOS_MATURITY_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/aumos_maturity
AUMOS_MATURITY_KAFKA_BOOTSTRAP_SERVERS=localhost:9092
AUMOS_MATURITY_KEYCLOAK_URL=http://localhost:8080
AUMOS_MATURITY_KEYCLOAK_REALM=aumos
AUMOS_MATURITY_KEYCLOAK_CLIENT_ID=maturity-assessment
AUMOS_MATURITY_KEYCLOAK_CLIENT_SECRET=change-me-in-production
AUMOS_MATURITY_REDIS_URL=redis://localhost:6379/0

# ── Maturity Assessment Specific ─────────────────────────────────────────────
AUMOS_MATURITY_SCORE_MAX_DIMENSION=100
AUMOS_MATURITY_SCORE_MATURITY_LEVELS=5
AUMOS_MATURITY_BENCHMARK_CACHE_TTL_SECONDS=3600
AUMOS_MATURITY_BENCHMARK_UPDATE_INTERVAL_DAYS=30
AUMOS_MATURITY_ROADMAP_MAX_INITIATIVES=20
AUMOS_MATURITY_ROADMAP_DEFAULT_HORIZON_MONTHS=18
AUMOS_MATURITY_PILOT_TARGET_SUCCESS_RATE=0.12
AUMOS_MATURITY_PILOT_DEFAULT_DURATION_WEEKS=8
AUMOS_MATURITY_REPORT_MAX_PAGES=50
AUMOS_MATURITY_REPORT_INCLUDE_BENCHMARKS=true

# ── Observability ─────────────────────────────────────────────────────────────
AUMOS_MATURITY_OTEL_ENDPOINT=http://localhost:4317
AUMOS_MATURITY_LOG_LEVEL=INFO
AUMOS_MATURITY_SENTRY_DSN=
